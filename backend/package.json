{
  "name": "app-name",
  "version": "1.0.0",
  "description": "App description",
  "main": "dist/app.js",
  "scripts": {
    "int-test": "cross-env mocha -r node_modules/ts-node/register \"test/integration/**/*.ts\" --reporter list --exit",
    "test": "cross-env mocha -r node_modules/ts-node/register \"test/**/*.ts\" --reporter list --exit",
    "test-file": "cross-env mocha -r node_modules/ts-node/register --reporter list",
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts",
    "serve": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "coverage": "nyc mocha \"test/**/*.ts\"",
    "swagger": "ts-node tasks/submit_swagger_to_s3.ts"
  },
  "author": "VuLQ",
  "dependencies": {
    "@sentry/node": "^5.17.0",
    "@types/ajv": "^1.0.0",
    "ajv": "^6.12.4",
    "async-lock": "^1.2.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-router-ts": "0.0.15",
    "fs-extra": "^7.0.0",
    "gql-ts": "^1.0.18",
    "ioredis": "^4.9.5",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "sha1": "^1.1.1",
    "sinon": "^7.4.1",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.1",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.1.0",
    "@types/ioredis": "^4.0.10",
    "@types/lodash": "^4.14.132",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.7",
    "@types/node-schedule": "^1.2.3",
    "@types/randomstring": "^1.1.6",
    "@types/request-promise": "^4.1.44",
    "@types/sha1": "^1.1.2",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1",
    "ts-node": "^7.0.1"
  }
}
